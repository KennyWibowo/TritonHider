{
  "name": "TritonHider",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Hides the useless links to deprecated pages on TritonLink",
  "permissions": ["tabs", "*://act.ucsd.edu/*"],
  "background":
  {
        "scripts": ["dependencies/jquery.min.js"]
  },
  "content_scripts": [
    {
      "matches": ["*://act.ucsd.edu/*"],
      "js": ["dependencies/jquery.min.js", "tritonhider.js"],
      "run_at":"document_end"
    }
  ]
}